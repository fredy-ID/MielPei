{% extends 'base.html.twig' %}

{% block title %}Command{% endblock %}

{% block body %}
    <h1>Command</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ command.id }}</td>
            </tr>
            <tr>
                <th>SerialNumber</th>
                <td>{{ command.serialNumber }}</td>
            </tr>
            <tr>
                <th>Invoice</th>
                <td>{{ command.invoice }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ command.etat }}</td>
            </tr>
            <tr>
                <th>DeliveryDateAt</th>
                <td>{{ command.deliveryDateAt ? command.deliveryDateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ command.createdAt ? command.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ command.updatedAt ? command.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('command_index') }}">back to list</a>

    <a href="{{ path('command_edit', {'id': command.id}) }}">edit</a>

    {{ include('command/_delete_form.html.twig') }}
{% endblock %}
